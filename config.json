{
    "dataset_name": "smallnorb",
    "val_split_strategy": "loio",
    "batch_size": 64,
    "val_fraction": 0.1,
    "model_size": "small",
    "model_path":"model.keras",
    "use_pretrained":false,

    "learning_rate": 3e-3,
    "lr_decay_rate": 0.96,
    "lr_decay_steps": 2e4,

    "loss_function": "spread_loss",
    "margin_schedule": "sigmoid",
    "optimizer":"adamw",
    "max_margin_steps": 5e4,

    "run_eagerly": false,
    "epochs":5

}